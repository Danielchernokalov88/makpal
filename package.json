{
  "name": "react-head-start",
  "version": "0.9.0",
  "description": "React App bootstrap with Server Side Rendering, Code Splitting, Hot Reloading, Redux and React Router",
  "author": "Greg Tillbrook",
  "engines": {
    "node": ">=8"
  },
  "scripts": {
    "deploy": "now",
    "start": "npm run prod",
    "dev": "npm-run-all --parallel dev:build dev:serve",
    "prod": "npm run prod:build && npm run prod:serve",
    "clean": "rimraf dist",
    "dev:build": "npm run clean && cross-env NODE_ENV=development parcel watch src/client/index.js --out-dir dist/bundles",
    "dev:build:css": "npm run clean && cross-env NODE_ENV=development parcel watch src/client/index.scss --out-dir dist/bundles",
    "dev:serve": "cross-env NODE_ENV=development npx nodemon --exec babel-node --ext js,jsx --delay 0.5 --watch src --watch test --inspect src/server/",
    "prod:build": "npm run clean && npm run prod:copy-files && npm run prod:build-server && npm run prod:build-client ",
    "prod:build-client": "cross-env NODE_ENV=production parcel build src/client/index.js --public-url / --out-dir dist/bundles --detailed-report",
    "prod:build-server": "cd src/ && cross-env npx babel '**/*.{js,jsx}' --no-babelrc --presets react,node8 --plugins transform-class-properties,syntax-dynamic-import --out-dir '../dist' --ignore test.js",
    "prod:copy-files": "cross-env npx copyfiles -u 1 'src/static/**/*' dist",
    "prod:serve": "cross-env NODE_ENV=production node dist/server/",
    "test": "npm run lint && npm run test:unit && npm run test:e2e",
    "testCI": "",
    "lint": "npx eslint --ext jsx,js src test",
    "test:unit": "npx jest --config test/unit-test/jest.config.js",
    "1test:e2e": "npm run prod:build && cross-env NODE_ENV=test node-while -s dist/server/index.js -r \"node node_modules/cypress/bin/cypress run\"",
    "1test:e2e:dev": "npx cypress open --env env=production",
    "1test:watch": "npm-watch",
    "1test:quick": "npm run lint && npm run test:unit",
    "test:e2e": "testee test.html --browsers=firefox --reporter=XUnit > testresults.xml",
    "todo": "npx fixme -i 'node_modules/**' -i '.git/**' -i 'dist/**' --skip note"
  },
  "browser": {
    "./src/client/codeSplitMappingsSync.js": "./src/client/codeSplitMappingsAsync.js",
    "config": "./src/client/utils/clientConfig.js"
  },
  "watch": {
    "test:quick": {
      "patterns": [
        "src",
        "test"
      ],
      "extensions": "js,jsx",
      "quiet": false
    }
  },
  "postcss": {
    "modules": false,
    "plugins": {
      "autoprefixer": {
        "browsers": [
          ">1%",
          "last 4 versions",
          "Firefox ESR",
          "not ie < 9"
        ],
        "flexbox": "no-2009"
      }
    }
  },
  "dependencies": {
    "@cycle/dom": "^20.2.0",
    "@cycle/history": "^6.10.0",
    "@cycle/http": "^14.9.0",
    "@cycle/isolate": "^3.2.0",
    "@cycle/run": "^4.1.0",
    "@cycle/storage": "^5.0.3",
    "@cycle/time": "^0.13.0",
    "apollo-client": "^1.9.1",
    "arangojs": "^6.4.0",
    "babel-preset-flow": "^6.23.0",
    "caller-callsite": "^2.0.0",
    "compression": "^1.7.3",
    "config": "^1.31.0",
    "cookie-parser": "^1.4.3",
    "cycle-onionify": "^5.0.0",
    "cycle-storageify": "^4.0.1",
    "cyclejs-utils": "^3.1.0",
    "cyclic-router": "^5.1.7",
    "express": "^4.16.3",
    "graphql": "^0.13.2",
    "graphql-anywhere": "^3.1.0",
    "graphql-fragments": "^0.1.0",
    "graphql-server-koa": "^1.4.0",
    "graphql-subscriptions": "^0.4.4",
    "graphql-tag": "^2.4.2",
    "graphql-tools": "^1.2.3",
    "jsonwebtoken": "^8.3.0",
    "jwt-decode": "^2.2.0",
    "koa": "^2.5.2",
    "koa-bodyparser": "^4.2.0",
    "koa-connect": "^2.0.1",
    "koa-logger": "^3.0.1",
    "koa-mount": "^3.0.0",
    "koa-router": "^7.2.1",
    "koa-static": "^5.0.0",
    "morgan": "^1.9.0",
    "ms": "^2.1.1",
    "persistgraphql": "^0.3.10",
    "ramda": "^0.25.0",
    "request-promise-native": "^1.0.5",
    "serve-favicon": "^2.5.0",
    "simple-oauth2": "^2.2.0",
    "subscriptions-transport-ws": "^0.8.2",
    "switch-path": "^1.2.0",
    "winston": "^2.4.3",
    "xstream": "^11.2.0"
  },
  "devDependencies": {
    "autoprefixer": "^7.2.1",
    "ava": "^0.25.0",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.0.3",
    "babel-jest": "^21.2.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-node8": "^1.2.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-preset-stage-3": "^6.24.1",
    "cheerio": "^1.0.0-rc.2",
    "copyfiles": "^1.2.0",
    "core-js": "^2.5.3",
    "cross-env": "^5.1.1",
    "cycle-restart": "^0.2.3",
    "cycle-scripts-one-fits-all": "5.4.3",
    "cyclejs-test-helpers": "^2.0.0",
    "cypress": "^1.4.0",
    "enzyme": "^3.2.0",
    "enzyme-adapter-react-16": "^1.1.0",
    "eslint": "^4.13.1",
    "eslint-plugin-jest": "^21.5.0",
    "eslint-plugin-react": "^7.5.1",
    "fixme": "^0.4.4",
    "github-markdown-css": "^2.10.0",
    "html-looks-like": "^1.0.3",
    "husky": "^0.14.3",
    "jest": "^21.2.1",
    "jest-junit": "^5.1.0",
    "jsverify": "^0.8.3",
    "lint-staged": "^7.0.3",
    "node-sass": "^4.7.2",
    "node-while": "^1.0.1",
    "nodemon": "^1.12.7",
    "npm-run-all": "^4.1.2",
    "npm-watch": "^0.3.0",
    "nyc": "^12.0.2",
    "parcel-bundler": "^1.6.2",
    "parcel-plugin-bundle-visualiser": "^1.0.2",
    "prettier": "^1.11.1",
    "prop-types": "^15.6.0",
    "regenerator-runtime": "^0.11.1",
    "rimraf": "^2.6.2",
    "snabbdom-to-html": "^5.1.0",
    "storybook-router": "^0.3.2",
    "wait-on": "^2.1.0"
  },
  "keywords": [
    "react",
    "bootstrap",
    "head start",
    "redux",
    "react-router",
    "express",
    "ssr",
    "server side render",
    "code splitting",
    "hot loading",
    "boilerplate"
  ],
  "ava": {
    "files": [
      "src/client/**/*.spec.js",
      "src/server/**/*.spec.js"
    ],
    "source": [
      "src/client/**/*.js",
      "src/server/**/*.js"
    ],
    "failFast": true,
    "babel": "inherit"
  },
  "nyc": {
    "include": [
      "src/client/**/*.js",
      "src/server/**/*.js"
    ],
    "exclude": [
      "**/*.spec.js",
      "test/**/*.spec.js",
      "client/reducers.js",
      "client/store.js",
      "client/routes.js",
      "server/util/setup-test-env.js",
      "server/util/test-helpers.js",
      "server/config.js",
      "server/dummyData.js"
    ],
    "reporter": [
      "lcov",
      "text",
      "html"
    ]
  },
  "repository": "gregtillbrook/react-head-start",
  "license": "MIT"
}
